<div>
<%= form_tag( "/qrcode/ui/#{@output_format}", id: "form", method: "post" ){ %>
  <table>
    <tr>
      <td><%= text_field_tag( :value, "", placeholder: "QRCODE VALUE" ) %></td>
      <td><%= submit_tag( "create" ) %></td>
    </tr>
    <tr>
      <td><%= text_field_tag( :qrcode_url, "", placeholder: "QRCODE URL" ) %></td>
      <td><%= submit_tag( "view" ) %></td>
    </tr>
  </table>
<% } %>
</div>
